<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.daul.jadwalsholat">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/sholat23"
            android:label="Jadwal Sholat"
            android:roundIcon="@mipmap/sholat23"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">

        <activity android:name="com.daul.jadwalsholat.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.daul.jadwalsholat.Activity_Jadwal"/>
        <activity android:name="com.daul.jadwalsholat.Activity_Halal"/>
        <activity android:theme="@style/AppTheme" android:name="com.daul.jadwalsholat.ActivityAsmaulHusna"/>
        <activity android:theme="@style/AppTheme.NoActionBar" android:name="com.daul.jadwalsholat.ActivityAsmaulHusnaDetail"/>
    </application>

</manifest>